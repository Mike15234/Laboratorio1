@model IEnumerable<Laboratorio1_ED2.Models.ListaComprimidos>

@{
    ViewBag.Title = "Listado";
}

<h2>Listado</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.NombreArchivo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RazonCompresio)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FactorCompresion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PorcentajeCompresion)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NombreArchivo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RazonCompresio)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FactorCompresion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PorcentajeCompresion)
        </td>
    </tr>
}

</table>
